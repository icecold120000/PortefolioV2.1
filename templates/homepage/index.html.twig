                <h3>Mes Réalisations</h3>
                <div class="container">
                    <div class="row">
                        {% for projet in projets %}
                            <div class="col-lg-4 col-sm-6">
                                <div class="projet-block">
                                    <p>{{ projet.nomProjet}} </p>
                                    <a href="{{path('projet_show', {'id': projet.id})}}" target="_blank">
                                        <img src="{{asset('uploads/VigProjet/' ~ projet.vignetteProjet)}}" class="img-thumbnail" alt="Image de projet">
                                    </a>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </section>
            <div>
                <a href="{{ path('projet_index') }}" class="btn btn-primary" id="btnRetour">
                    Voir tous les projets
                </a>
            </div>
        </section>
